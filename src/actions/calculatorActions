import { UPDATE_DISPLAY_VALUE } from "./types";

export const updateDisplayValue = () => dispatch => {
    console.log("action");
    dispatch({
        type: UPDATE_DISPLAY_VALUE,
        payload: '{name:"era"}'
    });
};

